<!DOCTYPE html>
<html lang="en">
<head>
    <script src="jquery/jquery.min.js"></script>
    <script src="pixi.js/pixi.js"></script>
    <script src="assets/javascript/common.js"></script>
    <script src="assets/javascript/components.js"></script>
    <script src="assets/javascript/meshDeformation.js"></script>
    <script src="assets/javascript/hitbox.js"></script>
    <script src="assets/javascript/Map.js"></script>
    <script src="assets/javascript/staticObjects.js"></script>
    <script src="assets/javascript/los.js"></script>
    <script src="assets/javascript/roof.js"></script>
    <script src="assets/javascript/filesystem.js"></script>
    <script src="assets/javascript/spells.js"></script>
    <script src="assets/javascript/debug.js"></script>
    <script src="assets/javascript/runaround.js"></script>
    <script src="assets/javascript/wallSectionsRenderer.js"></script>
    <script src="assets/javascript/rendering.js"></script>
    <link rel="icon" type="image/png" href="assets/images/wizard-hat-favicon.png">
    <link rel="stylesheet" href="assets/css/survivor.css">
    <link rel="stylesheet" href="assets/css/dialogBoxes.css">
</head>
<body>
    <div id="msgbox" class="hidden dialogBox"><div id="msgbuttons"></div></div>
    <div id="time"></div>
    <div id="statusBars">
        <div class="statusBar">
            <div class="statusIcon"><img src="assets/images/red heart.png"></div>
            <div class="statusBar-background">
                <div id="healthBar" class="statusBar-fill"></div>
            </div>
        </div>
        <div class="statusBar">
            <div class="statusIcon"><img src="assets/images/beaker.png"></div>
            <div class="statusBar-background">
                <div id="magicBar" class="statusBar-fill"></div>
            </div>
        </div>
    </div>
    <div id="auraSelector">
        <div id="selectedAura"></div>
        <div id="activeAuraIcons" class="hidden"></div>
        <div id="auraMenu" class="hidden">
            <div id="auraGrid"></div>
        </div>
    </div>
    <div id="spellSelector">
        <div id="selectedSpell"></div>
        <div id="spellMenu" class="hidden">
            <div id="spellGrid"></div>
        </div>
    </div>

    <div id="display"></div>
    <div id="msg"></div>
</body>
